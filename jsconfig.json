{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      // Your custom aliases here
      "assets/*": [
        "./assets/*"
      ],
      "data/*": [
        "./data/*"
      ],
      "features/*": [
        "./components/features/*"
      ],
      "layout/*": [
        "./components/layout/*"
      ],
      "screens/*": [
        "./components/screens/*"
      ],
      "components/*": [
        "./components/*"
      ],
      "reducers/*": [
        "./reducers/*"
      ],
    },
  },
  "exclude": ["node_modules"],
}
